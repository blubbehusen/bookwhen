{
	"name" : "inshore/bookwhen",
	"description" : "Bookwhen API SDK",
	"keywords" : [
		"api",
		"bookwhen",
		"inShore",
		"package"
	],
	"homepage" : "https://github.com/inshore/bookwhen",
	"license" : "MIT",
	"authors" : [{
			"name" : "Daniel Mullin",
			"email" : "email@danielmullin.com"
		}
	],
	"require" : {
		"php" : ">=7.2",
		"guzzlehttp/guzzle" : "^7.0",
		"respect/validation" : "^2.0",
		"monolog/monolog": "^2.1"
	},
	"require-dev" : {
		"phpunit/phpunit" : "4.*"
	},
	"autoload" : {
		"psr-4" : {
			"InShore\\Bookwhen\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"InShore\\Bookwhen\\Test\\" : "tests"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.0-dev"
		}
	}
}
